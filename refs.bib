	keywords = {Green Fluorescent Proteins, HeLa Cells, Humans, Animals, Kinetics, Recombinant Fusion Proteins, Cell Compartmentation, Proteolysis, Anaphase-Promoting Complex-Cyclosome, Indoleacetic Acids, Lysine, Single-Domain Antibodies, Zebrafish},
	pages = {3297}
}

